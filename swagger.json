  "components": {
    "schemas": {
      "AspNetCore.Mvc.ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "DAL.Enums.StatusTransacaoEnum": {
        "enum": [
          "1 - BOLETO_REGISTRADO",
          "2 - PAGO",
          "3 - BOLETO_LIQUIDADO_COMPENSADO",
          "4 - CANCELADO",
          "5 - EM_PROCESSO",
          "6 - AUTORIZADO",
          "7 - AGUARDANDO_PAGAMENTO",
          "8 - ESTORNADO",
          "9 - AGUARDANDO_ESTORNO",
          "10 - RECUSADO",
          "11 - DEVOLVIDO",
          "12 - ANALISE",
          "13 - REVISAO_PAGAMENTO",
          "14 - SUSPENSO",
          "15 - EM_DISPUTA",
          "16 - TRANSFERIDO",
          "17 - NAO_PAGO",
          "18 - FINALIZADO",
          "19 - CONFIRMADO",
          "20 - REJEITADO"
        ],
        "type": "string",
        "format": "int32"
      },
      "DAL.Enums.TipoChaveTransferencia": {
        "enum": [
          "1 - TELEFONE",
          "2 - EMAIL",
          "3 - DOCUMENTO",
          "4 - CHAVE_ALEATÓRIA"
        ],
        "type": "string",
        "format": "int32"
      },
      "DAL.Enums.TipoOperacao": {
        "enum": [
          "1 - BOLETO",
          "2 - CARTAO_DEBITO",
          "3 - CARTAO_CREDITO",
          "4 - PIX",
          "5 - DEPOSITO",
          "6 - JUROS",
          "7 - TRANSFERENCIA_EXTERNA_PIX",
          "8 - TRANSFERENCIA_INTERNA",
          "9 - PAGAMENTO_BOLETO",
          "10 - PAGAMENTO_PIX",
          "11 - TRANSFERENCIA_EXTERNA_TEDTEV",
          "12 - DEPOSITO_USDT",
          "13 - DEPOSITO_PESO"
        ],
        "type": "string",
        "format": "int32"
      },
      "TransacaoLinkDto.RequestCreateLinkThreeds": {
        "required": [
          "valor"
        ],
        "type": "object",
        "properties": {
          "valor": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "phone": {
            "maxLength": 13,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "postbackUrl": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "metadata": {
            "maxLength": 1000,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Unknown": {
        "type": "object",
        "properties": {
          "transacaoId": {
            "type": "integer",
            "format": "int64"
          },
          "nossoNumero": {
            "type": "string",
            "nullable": true
          },
          "valor": {
            "type": "number",
            "format": "double"
          },
          "valorBruto": {
            "type": "number",
            "format": "double"
          },
          "statusAnterior": {
            "$ref": "#/components/schemas/DAL.Enums.StatusTransacaoEnum"
          },
          "statusAtual": {
            "$ref": "#/components/schemas/DAL.Enums.StatusTransacaoEnum"
          },
          "tentativaAtual": {
            "type": "integer",
            "format": "int32"
          },
          "metaData": {
            "type": "string",
            "nullable": true
          },
          "tipoTransacao": {
            "$ref": "#/components/schemas/DAL.Enums.TipoOperacao"
