    "/transacao": {
      "get": {
        "tags": [
          "Transacao"
        ],
        "summary": "Listar Transações",
        "description": "Retorna a lista de transações no período informado, respeitando os parâmetros de filtragem.",
        "parameters": [
          {
            "name": "PeriodoInicio",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "PeriodoFim",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "Pagina",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "RegistrosPorPagina",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "Status",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Recurso obtido com sucesso.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/api.net.DTOs.Transacao.ResponseTransacaoList"
