  "paths": {
    "/carteira/cashout": {
      "post": {
        "tags": [
          "Carteira"
        ],
        "summary": "Realizar Transferência - Pix",
        "description": "Permite realizar uma transferência via pix com os dados informados.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/api.net.DTOs.Transacao.RequestCreateCashout"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Recurso criado com sucesso.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/api.net.DTOs.Transacao.ResponseTransacao"
                }
              }
            }
          },
          "400": {
            "description": "Algum parâmetro obrigratório não foi passado, ou não estão corretos.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/api.net.Interceptors.BadRequest.BadRequestErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "Ausência de credenciais do usuário na requisição."
          },
          "422": {
            "description": "Erro de validação da lógica de negocio, a mensagem será retornada na resposta.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/api.net.DTOs.BasicErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Erro interno do EcommerceBank, tente novamente. Caso persista o erro, entrar em contato com o time de desenvolvimento."
          }
        }
      }
    },
